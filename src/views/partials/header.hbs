<header class="members-header container">
  {{#if username}}
    <div class="user-info">
      <img src="{{avatar_url}}" alt="{{username}}'s profile picture">
      <strong>{{username}}</strong>
    </div>
  {{/if}}
  <h1>{{title}}</h1>
  <p>{{description}}</p>
  <ul class="menu">
    <li><a href="/">FAC Members</a></li>
    <li><a href="/blog/posts">Blog Posts</a></li>
  </ul>
</header>
